parserImport XOCL;
parserImport XEditor;

import Diagram;

@EventHandler diagramMenuEvent(name:String,menu:[String])

  // The link has been clicked on the named diagram. Handle the link and
  // return the SVG to update the diagram...
  
    try
      handleMenu(name,menu)
    catch(exception)
      Root::xmfException := exception;
      exception.printBacktrace();
      null
    end
  end;